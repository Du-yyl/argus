{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "fs:default",
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        {
          "path": "D:/*"
        }
      ]
    },
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "D:/*"
        }
      ]
    },
    {
      "identifier": "allow-read-dir",
      "allow": [
        {
          "path": "D:/*"
        }
      ]
    },
    {
      "identifier": "fs:read-files",
      "allow": [
        {
          "path": "D:/*"
        }
      ]
    },
    "core:default",
    "shell:allow-open",
    "fs:default",
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:window:allow-set-title"
  ],
  "capabilities": [
  ]
}
