<UserControl
    x:Class="UI.Views.Setting.GeneralSettingView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:UI.Views.Setting"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:viewModels="clr-namespace:UI.ViewModels"
    xmlns:model="clr-namespace:UI.Model"
    xmlns:setting="clr-namespace:UI.ViewModels.Setting"
    prism:ViewModelLocator.AutoWireViewModel="True"
    d:DataContext="{d:DesignInstance setting:GeneralSettingViewModel, IsDesignTimeCreatable=True}"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="300">
    <Grid>
        <TextBlock>
            基础设置 
            
            主要包含：
                语言
                地图相关选项
                计量单位（）
                缩略图
        </TextBlock>
    </Grid>
</UserControl>
